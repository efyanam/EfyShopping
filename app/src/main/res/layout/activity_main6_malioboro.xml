<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gambar"
    tools:context="com.example.anas.myapplication1.Main6Malioboro">
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            android:src="@drawable/malioboro2" />
        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="match_parent"
            android:layout_marginTop="250dp"
            android:layout_height="250dp"
            android:src="@drawable/malioboro1" />
        <TextView
            android:id="@+id/textViewLinkmalioboro"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/color1"
            android:layout_marginTop="900dp"
            android:textSize="15sp"
            android:textStyle="bold"
            android:background="@color/color7"
            android:text="LihatPeta" />
        <TextView
            android:layout_width="396dp"
            android:layout_marginTop="510dp"
            android:layout_height="match_parent"
            android:text="@string/Malioboro"
            android:textSize="15sp"
            android:layout_marginLeft="10dp"
            android:textStyle="bold"
            android:textColor="@color/color8"
            android:fontFamily="casual"
            android:id="@+id/Malioboro"/>


    </FrameLayout>

</ScrollView>
